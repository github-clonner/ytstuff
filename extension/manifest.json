{
  "manifest_version": 2,

  "name": "yt songs recorder",
  "version": "1.0",

  "browser_action": {
    "default_popup": "index.html"
  },

  "background": {
    "scripts": ["socket.io-client.js", "background.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "https://www.youtube.com/"
  ]
}